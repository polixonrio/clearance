<!DOCTYPE html>
<html>
  <head>
    <title>Select Students</title>

    <link rel="stylesheet" href="stylesheets/output.css" />
    <link rel="icon" type="image/x-icon" href="/images/argi.webp">

    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  </head>
  <body>
    <div>
      <div>
        <div>
          <h1 class="text-2xl">Students</h1>
          <div>
            <a href="/logout">Logout</a>
            <a href="/filter">Back to Filter</a>
          </div>
          <div class="w-11/12 mx-auto">
          <div class="relative overflow-x-auto">
            <table class="w-full text-sm text-left text-gray-500 dark:text-gray-400">
                <thead class="text-xs  text-white uppercase bg-gray-800 dark:bg-gray-700 dark:text-gray-400">
                    <tr>
                        <th scope="col" class="px-6 py-3">
                            Registration Number
                        </th>
                        <th scope="col" class="px-6 py-3">
                            Name
                        </th>
                        <th scope="col" class="px-6 py-3">
                            Course
                        </th>
                        <th scope="col" class="px-6 py-3">
                            Semester
                        </th>
                        <th scope="col" class="px-6 py-3">
                            Branch
                        </th>
                        <th scope="col" class="px-6 py-3">
                            Section
                        </th>
                        <th scope="col" class="px-6 py-3">
                            Session
                        </th>
                        <th scope="col" class="px-6 py-3">
                            Year
                        </th>
                        <th scope="col" class="px-6 py-3">
                            Mobile Number
                        </th>
                        <th scope="col" class="px-6 py-3">
                            Verification Status
                        </th>
                        <th scope="col" class="px-6 py-3">
                            Action
                        </th>
                    </tr>
                </thead>
                <tbody>
                    <% if (typeof students !== 'undefined' && students.length > 0) { %>
                        <% students.forEach(function(student) { %>
                            <tr class="bg-white border-b dark:bg-gray-800 dark:border-gray-700">
                                <td class="px-6 py-4 font-medium text-gray-900 whitespace-nowrap dark:text-white">
                                    <%= student.Registration_Number %>
                                </td>
                                <td class="px-6 py-4">
                                    <%= student.Name %>
                                </td>
                                <td class="px-6 py-4">
                                    <%= student.Course %>
                                </td>
                                <td class="px-6 py-4">
                                    <%= student.Semester %>
                                </td>
                                <td class="px-6 py-4">
                                    <%= student.Branch %>
                                </td>
                                <td class="px-6 py-4">
                                    <%= student.Section %>
                                </td>
                                <td class="px-6 py-4">
                                    <%= student.Session %>
                                </td>
                                <td class="px-6 py-4">
                                    <%= student.Year %>
                                </td>
                                <td class="px-6 py-4">
                                    <%= student.Mobile_Number %>
                                </td>
                                <td class="px-6 py-4">
                                    <% if (typeof student.Accounts_Verified !== 'undefined') { %>
                                        <% if (student.Accounts_Verified === 0) { %>
                                            UNVERIFIED
                                        <% } else if (student.Accounts_Verified === 1) { %>
                                            VERIFIED
                                        <% } %>
                                    <% } else if (typeof student.Library_Verified !== 'undefined') { %>
                                        <% if (student.Library_Verified === 0) { %>
                                            UNVERIFIED
                                        <% } else if (student.Library_Verified === 1) { %>
                                            VERIFIED
                                        <% } %>
                                    <% } else if (typeof student['Training_And_Placement Verified'] !== 'undefined') { %>
                                        <% if (student['Training_And_Placement Verified'] === 0) { %>
                                            UNVERIFIED
                                        <% } else if (student['Training_And_Placement Verified'] === 1) { %>
                                            VERIFIED
                                        <% } %>
                                    <% } else if (typeof student.Alumni_Cell_Verified !== 'undefined') { %>
                                        <% if (student.Alumni_Cell_Verified === 0) { %>
                                            UNVERIFIED
                                        <% } else if (student.Alumni_Cell_Verified === 1) { %>
                                            VERIFIED
                                        <% } %>
                                    <% } else if (typeof student.Project_Verified !== 'undefined') { %>
                                        <% if (student.Project_Verified === 0) { %>
                                            UNVERIFIED
                                        <% } else if (student.Project_Verified === 1) { %>
                                            VERIFIED
                                        <% } %>
                                    <% } else if (typeof student.Internship_Verified !== 'undefined') { %>
                                        <% if (student.Internship_Verified === 0) { %>
                                            UNVERIFIED
                                        <% } else if (student.Internship_Verified === 1) { %>
                                            VERIFIED
                                        <% } %>
                                    <% } else if (typeof student.HOD_Verified !== 'undefined') { %>
                                        <% if (student.HOD_Verified === 0) { %>
                                            UNVERIFIED
                                        <% } else if (student.HOD_Verified === 1) { %>
                                            VERIFIED
                                        <% } %>
                                    <% } %>
                                </td>
                                <td class="px-6 py-4">
                                    <% if (typeof student.Accounts_Verified !== 'undefined') { %>
                                        <% if (student.Accounts_Verified === 0) { %>
                                            <form action="/students/<%= student.Registration_Number %>/verify" method="POST">
                                                <button type="submit">Verify</button>
                                            </form>
                                        <% } else if (student.Accounts_Verified === 1) { %>
                                            <form action="/students/<%= student.Registration_Number %>/unverify" method="POST">
                                                <button type="submit">Unverify</button>
                                            </form>
                                        <% } %>
                                    <% } else if (typeof student.Library_Verified !== 'undefined') { %>
                                        <% if (student.Library_Verified === 0) { %>
                                            <form action="/students/<%= student.Registration_Number %>/verify" method="POST">
                                                <button type="submit">Verify</button>
                                            </form>
                                        <% } else if (student.Library_Verified === 1) { %>
                                            <form action="/students/<%= student.Registration_Number %>/unverify" method="POST">
                                                <button type="submit">Unverify</button>
                                            </form>
                                        <% } %>
                                    <% } else if (typeof student['Training_And_Placement Verified'] !== 'undefined') { %>
                                        <% if (student['Training_And_Placement Verified'] === 0) { %>
                                            <form action="/students/<%= student.Registration_Number %>/verify" method="POST">
                                                <button type="submit">Verify</button>
                                            </form>
                                        <% } else if (student['Training_And_Placement Verified'] === 1) { %>
                                            <form action="/students/<%= student.Registration_Number %>/unverify" method="POST">
                                                <button type="submit">Unverify</button>
                                            </form>
                                        <% } %>
                                    <% } else if (typeof student.Alumni_Cell_Verified !== 'undefined') { %>
                                        <% if (student.Alumni_Cell_Verified === 0) { %>
                                            <form action="/students/<%= student.Registration_Number %>/verify" method="POST">
                                                <button type="submit">Verify</button>
                                            </form>
                                        <% } else if (student.Alumni_Cell_Verified === 1) { %>
                                            <form action="/students/<%= student.Registration_Number %>/unverify" method="POST">
                                                <button type="submit">Unverify</button>
                                            </form>
                                        <% } %>
                                    <% } else if (typeof student.Project_Verified !== 'undefined') { %>
                                        <% if (student.Project_Verified === 0) { %>
                                            <form action="/students/<%= student.Registration_Number %>/verify" method="POST">
                                                <button type="submit">Verify</button>
                                            </form>
                                        <% } else if (student.Project_Verified === 1) { %>
                                            <form action="/students/<%= student.Registration_Number %>/unverify" method="POST">
                                                <button type="submit">Unverify</button>
                                            </form>
                                        <% } %>
                                    <% } else if (typeof student.Internship_Verified !== 'undefined') { %>
                                        <% if (student.Internship_Verified === 0) { %>
                                            <form action="/students/<%= student.Registration_Number %>/verify" method="POST">
                                                <button type="submit">Verify</button>
                                            </form>
                                        <% } else if (student.Internship_Verified === 1) { %>
                                            <form action="/students/<%= student.Registration_Number %>/unverify" method="POST">
                                                <button type="submit">Unverify</button>
                                            </form>
                                        <% } %>
                                    <% } else if (typeof student.HOD_Verified !== 'undefined') { %>
                                        <% if (student.HOD_Verified === 0) { %>
                                            <form action="/students/<%= student.Registration_Number %>/verify" method="POST">
                                                <button type="submit">Verify</button>
                                            </form>
                                        <% } else if (student.HOD_Verified === 1) { %>
                                            <form action="/students/<%= student.Registration_Number %>/unverify" method="POST">
                                                <button type="submit">Unverify</button>
                                            </form>
                                        <% } %>
                                    <% } %>
                                </td>
                            </tr>
                        <% }); %> <% } else { %>
                            <tr>
                                <td colspan="7">No students found.</td>
                            </tr>
                        <% } %>
                    </tbody>
                </table>
            </div>
          </div>
        </div>
        
      
        </div>
      </div>
    </div>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/flowbite/1.6.5/flowbite.min.js"></script>

  </body>
  
</html>
