<!DOCTYPE html>
<html>
  <head>
    <title>Admin Dashboard</title>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="stylesheets/output.css" />

  
  </head>
  <body>
    

    
<nav class="bg-gray-800 shadow-xl  fixed w-full z-20 top-0 left-0 border-b ">
  <div class="max-w-screen-xl flex flex-wrap items-center justify-between mx-auto p-4">
  <a href="/" class="flex items-center">
      <img src="images/argi.webp" class="md:h-12 h-8 mr-3" alt="Flowbite Logo">
      <span class="self-center text-white text-base md:text-2xl font-semibold whitespace-nowrap">Raisoni Clearance System</span>
  </a>
  <div class="flex md:order-2">
     <a href="/logout"> <button type="button" class="text-white bg-blue-700 hover:bg-blue-800 focus:ring-4 focus:outline-none focus:ring-blue-300 font-medium rounded-lg text-sm px-4 py-2 text-center mr-3 md:mr-0 dark:bg-blue-600 dark:hover:bg-blue-700 dark:focus:ring-blue-800">Logout</button></a>

  </div>
  <div class="items-center justify-between hidden w-full md:flex md:w-auto md:order-1" id="navbar-sticky">
    <ul class="flex flex-col p-4 md:p-0 mt-4 font-medium border border-gray-100 rounded-lg bg-gray-50 md:flex-row md:space-x-8 md:mt-0 md:border-0 md:bg-white dark:bg-gray-800 md:dark:bg-gray-900 dark:border-gray-700">
     
    </ul>
  </div>
  </div>
</nav>




      <section class="bg-white-100 mt-20 md:mt-36">
        <div
          class="flex flex-col text-center items-center px-6 py-8 mx-auto justify-center lg:py-0"
        >
          <div
            class="w-full bg-white-100 rounded-lg shadow md:mt-0 sm:max-w-md xl:p-0"
          >
            <div class="p-6 space-y-4 md:space-y-6 sm:p-8">
              <h1
                class="text-xl font-bold leading-tight tracking-tight text-gray-900 md:text-2xl"
              >
                Admin System
              </h1>
              <h1
                class="text-xl font-bold leading-tight tracking-tight text-gray-900 md:text-2xl"
              >
                Upload Student Data
              </h1>
              <h1
                class="text-xl font-bold leading-tight tracking-tight text-gray-900 md:text-2xl"
              >
                ( Excel File xlsx Format Only)
              </h1>
              <% if (typeof errorFlashMessage !== 'undefined' && errorFlashMessage.length > 0) { %>
                <h3 class="text-base bg-white-100 font-bold leading-tight tracking-tight text-red-600 md:text-xl dark:text-red"><%= errorFlashMessage[0] %>  </h3>
              
              
              <% } %>
              <% if (typeof successFlashMessage !== 'undefined' && successFlashMessage.length > 0) { %>
                <h3 class="text-base bg-white-100 font-bold leading-tight tracking-tight text-red-600 md:text-xl dark:text-red"><%= successFlashMessage[0] %>  </h3>
              
              
              <% } %>
              
              <form
                class="space-y-4 md:space-y-6"
                action="/upload"
                method="post"
                enctype="multipart/form-data"
              >
                <input type="file" name="myfile" class="focus:bg-gray-100 border-gray-700 border-4 focus:text-black" accept=".xlsx" required />
                <div class=""><input type="submit" class="px-1 bg-black focus:border focus:border-indigo-700 text-white focus:bg-gray-100 focus:text-black px-4 py-2" value="Upload File" /></div>
              </form>
            </div>
          </div>
        </div>
      </section>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/flowbite/1.6.5/flowbite.min.js"></script>
    <script>

    </script>  
  </body>
</html>
